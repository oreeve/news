<!DOCTYPE html>
<html>
  <head>
    <title>Articles</title>
  </head>

  <body>
    <h1>Articles</h1>

    <ol>
      <% @articles.each do |article| %>
        <li><%=article[:article_title]%><br>
        <a href="<%=article[:article_url]%>"><%=article[:article_url]%></a><br>
        <%=article[:article_desc]%><br>
      </li><br>
      <% end %>
    </ol>
  </body>
</html>
